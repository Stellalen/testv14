<odoo>

    <record id="customer_product_name_view_form" model="ir.ui.view">
        <field name="name">add customer MPN</field>
        <field name="model">product.customer.name</field>
        <field name="inherit_id" ref="ac_sale_product_customer_name.customer_product_name_view_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='name']" position="after">
                <field name="product_code"/>
            </xpath>

        </field>
    </record>

</odoo>